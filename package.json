{
  "name": "nextext-editor-monorepo",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter nextext-editor/demo dev",
    "build": "pnpm -r build",
    "build:editor": "pnpm --filter nextext-editor build",
    "build:demo": "pnpm --filter nextext-editor/demo build",
    "preview": "pnpm --filter nextext-editor/demo preview",
    "lint": "pnpm -r lint",
    "analyze": "pnpm --filter nextext-editor build:analyze",
    "clean": "rm -rf packages/*/dist apps/*/dist node_modules/.cache"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  }
}
